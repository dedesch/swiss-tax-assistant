{
    "name": "swiss-tax-assistant",
    "version": "1.0.0",
    "description": "Swiss Tax Declaration Assistant - Full Stack Application",
    "main": "api/calculations.js",
    "type": "module",
    "scripts": {
        "build:css": "npx tailwindcss -i ./src/input.css -o ./public/css/tailwind.css --watch",
        "build:css:prod": "npx tailwindcss -i ./src/input.css -o ./public/css/tailwind.css --minify",
        "dev": "npm run build:css:prod && vercel dev",
        "build": "npm run build:css:prod",
        "start": "vercel dev",
        "deploy": "npm run build:css:prod && vercel --prod"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.58.0",
        "autoprefixer": "^10.4.21",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.6.0",
        "multer": "^2.0.2",
        "openai": "^5.22.0",
        "pg": "^8.16.3",
        "playwright-aws-lambda": "^0.10.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.17"
    },
    "devDependencies": {
        "@types/node": "^20.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "swiss",
        "tax",
        "declaration",
        "assistant",
        "vercel",
        "serverless"
    ],
    "author": "Swiss Tax Assistant",
    "license": "MIT"
}
